	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[alias]
	bb = "!/home/osbm/Documents/dotfiles/scripts/better-git-branch.sh"
[column]
	ui = auto
[branch]
	sort = -committerdate
